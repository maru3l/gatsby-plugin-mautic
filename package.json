{
  "name": "gatsby-plugin-mautic",
  "version": "0.0.0",
  "author": "Marc-Antoine Ruel <hello@marcantoineruel.com>",
  "description": "Add Mautic tracking code to your Gatsby site.",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "google analytics"
  ],
  "license": "MIT",
  "homepage": "https://github.com/maru3l/gatsby-plugin-mautic#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maru3l/gatsby-plugin-mautic.git"
  },
  "bugs": {
    "url": "https://github.com/maru3l/gatsby-plugin-mautic/issues"
  },
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "minimatch": "3.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/core": "^13.2.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby-package": "^0.3.1",
    "commitlint-config-gitmoji": "^1.0.1",
    "cross-env": "^5.2.1",
    "cz-emoji": "^1.2.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "prettier": "2.0.4",
    "semantic-release-gitmoji": "^1.3.3"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": "eslint --cache --fix",
    "*.{md,html,css,json}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  }
}
